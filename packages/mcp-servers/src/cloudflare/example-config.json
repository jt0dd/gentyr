{
  "mcpServers": {
    "cloudflare-dns": {
      "command": "node",
      "args": [
        "/absolute/path/to/xy/packages/mcp-servers/dist/cloudflare/server.js"
      ],
      "env": {
        "CLOUDFLARE_API_TOKEN": "your-cloudflare-api-token-here",
        "CLOUDFLARE_ZONE_ID": "your-zone-id-here"
      }
    }
  }
}
